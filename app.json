{
  "expo": {
    "name": "Bazaario",
    "slug": "Bazaario",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon2.png",
    "scheme": "voidexiled.bazaario",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#1a1a1a",
      "bundleIdentifier": "com.voidexiled.bazaario"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon2.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.voidexiled.bazaario"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon2.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "244ce046-84f8-4fa9-893b-d889d809697b"
      }
    },
    "owner": "voidexiled"
  }
}